{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "imgs/icon-19.png",
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__"
  },
  "default_locale": "zh_CN",
  "description": "瀑布用得好，工作烦恼少",
  "icons": {
    "128": "imgs/icon-128.png",
    "16": "imgs/icon-16.png",
    "32": "imgs/icon-32.png"
  },
  "permissions": [
    "alarms",
    "cookies",
    "notifications",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*.mp.weixin.qq.com/*"
  ],
  "version": "0.1.2",
  "web_accessible_resources": [
    "template.html",
    "imgs/icon-32.png",
    "imgs/loading.gif"
  ]
}
